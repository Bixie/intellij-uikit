<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="Uikit-custom">
    <template name="uk-list"
            value="&#60;ul class=&quot;uk-list&quot;&#62;&#10;    &#60;li&#62;$END$&#60;/li&#62;&#10;&#60;/ul&#62;&#10;"
            description="UIkit uk-list component" toReformat="false">

        <context>
            <option name="HTML_TEXT" value="true"/>
            <option name="HTML" value="true"/>
            <option name="PHP" value="true"/>
            <option name="GSP" value="true"/>
            <option name="JSP" value="true"/>
        </context>
    </template>
    <template name="uk-button:button"
            value="&#10;&#60;button type=&quot;$buttontype$&quot; class=&quot;$class1$&quot;&#62;$content1$&#60;/button&#62;$END$&#10;"
            description="UIkit uk-button button" toReformat="false">
        <variable name="buttontype" expression="&quot;button&quot;" defaultValue="" alwaysStopAt="true"/>
        <variable name="class1" expression="&quot;uk-button&quot;" defaultValue="" alwaysStopAt="true"/>
        <variable name="content1" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="HTML_TEXT" value="true"/>
            <option name="HTML" value="true"/>
            <option name="PHP" value="true"/>
            <option name="GSP" value="true"/>
            <option name="JSP" value="true"/>
        </context>
    </template>
    <template name="uk-button:a"
            value="&#10;&#60;a href=&quot;$href$&quot; class=&quot;$class1$&quot;&#62;$content1$&#60;/a&#62;$END$&#10;"
            description="UIkit uk-button anchor" toReformat="false">
        <variable name="href" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="class1" expression="&quot;uk-button&quot;" defaultValue="" alwaysStopAt="true"/>
        <variable name="content1" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="HTML_TEXT" value="true"/>
            <option name="HTML" value="true"/>
            <option name="PHP" value="true"/>
            <option name="GSP" value="true"/>
            <option name="JSP" value="true"/>
        </context>
    </template>
    <template name="uk-slideshow-slidenav"
            value="&#60;div data-uk-slideshow=&quot;&quot;&#62;&#10;    &#60;div class=&quot;uk-slidenav-position&quot;&#62;&#10;        &#60;ul class=&quot;uk-slideshow&quot;&#62;&#10;            &#60;li&#62;&#60;img src=&quot;$imgsrc$&quot; alt=&quot;$alt$&quot;&#62;&#60;/li&#62;&#10;        &#60;/ul&#62;&#10;        &#60;ul&#62;&#10;            &#60;a href=&quot;&quot; class=&quot;uk-slidenav uk-slidenav-contrast uk-slidenav-previous&quot; data-uk-slideshow-item=&quot;previous&quot;&#62;&#60;/a&#62;&#10;            &#60;a href=&quot;&quot; class=&quot;uk-slidenav uk-slidenav-contrast uk-slidenav-next&quot; data-uk-slideshow-item=&quot;next&quot;&#62;&#60;/a&#62;&#10;        &#60;/ul&#62;&#10;    &#60;/div&#62;&#10;&#60;/div&#62;&#10;"
            description="UIkit uk-slideshow with slidenav" toReformat="false">
        <variable name="$imgsrc$" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="$alt$" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="HTML_TEXT" value="true"/>
            <option name="HTML" value="true"/>
            <option name="PHP" value="true"/>
            <option name="GSP" value="true"/>
            <option name="JSP" value="true"/>
        </context>
    </template>
    <template name="uk-slideshow-slider"
            value="&#10;&#60;div data-uk-slideshow=&quot;&quot;&#62;&#10;    &#60;div class=&quot;uk-slidenav-position&quot; data-uk-slider=&quot;{center:true}&quot;&#62;&#10;        &#60;div class=&quot;uk-slider-container&quot;&#62;&#10;            &#60;ul class=&quot;uk-slider uk-grid-width-medium-1-4&quot;&#62;&#10;                &#60;li data-uk-slideshow-item=&quot;0&quot;&#62;&#60;img src=&quot;$thumbsrc$&quot; alt=&quot;$alt$&quot;&#62;&#60;/li&#62;&#10;            &#60;/ul&#62;&#10;            &#60;a href=&quot;&quot; class=&quot;uk-slidenav uk-slidenav-contrast uk-slidenav-previous&quot; data-uk-slider-item=&quot;previous&quot;&#62;&#60;/a&#62;&#10;            &#60;a href=&quot;&quot; class=&quot;uk-slidenav uk-slidenav-contrast uk-slidenav-next&quot; data-uk-slider-item=&quot;next&quot;&#62;&#60;/a&#62;&#10;        &#60;/div&#62;&#10;    &#60;/div&#62;&#10;    &#60;div class=&quot;uk-slidenav-position&quot;&#62;&#10;        &#60;ul class=&quot;uk-slideshow&quot;&#62;&#10;            &#60;li&#62;&#60;img src=&quot;$imgsrc$&quot; alt=&quot;$alt$&quot;&#62;&#60;/li&#62;&#10;        &#60;/ul&#62;&#10;        &#60;ul&#62;&#10;            &#60;a href=&quot;&quot; class=&quot;uk-slidenav uk-slidenav-contrast uk-slidenav-previous&quot; data-uk-slideshow-item=&quot;previous&quot;&#62;&#60;/a&#62;&#10;            &#60;a href=&quot;&quot; class=&quot;uk-slidenav uk-slidenav-contrast uk-slidenav-next&quot; data-uk-slideshow-item=&quot;next&quot;&#62;&#60;/a&#62;&#10;        &#60;/ul&#62;&#10;    &#60;/div&#62;&#10;&#60;/div&#62;&#10;"
            description="UIkit Slideshow and slider components" toReformat="false">
        <variable name="$thumbsrc$" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="$alt$" expression="" defaultValue="" alwaysStopAt="true"/>
        <variable name="$imgsrc$" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="HTML_TEXT" value="true"/>
            <option name="HTML" value="true"/>
            <option name="PHP" value="true"/>
            <option name="GSP" value="true"/>
            <option name="JSP" value="true"/>
        </context>
    </template>
</templateSet>